[flake8]
ignore =
    E121,
    E126,
    E127,
    E128,
    E203,
    E225,
    E226,
    E231,
    E241,
    E251,
    E261,
    E265,
    E302,
    E303,
    E305,
    E402,
    E501,
    E741,
    W291,
    W292,
    W293,
    W391,
    W503,
    W504,
    F403,
    F405,
    B007,
    B950,
    C901,
exclude =
    .git,
    .venv,
    __pycache__,
    docs/source/conf.py,
    frontend,
    node-modules,
    build,
    dist,
    migrations
max-complexity = 10